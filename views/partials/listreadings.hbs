<table class="ui table">
  <thead>
    <tr>
      <th>Code</th>
      <th>Temperature</th>
      <th>Wind Speed</th>
    </tr>
  </thead>
  <tbody>
    {{#each station.readings}}
    <tr>
      <td>
        {{title}}
      </td>
      <td>
        {{code}}
      </td>
      <td>
      {{temperature}}
      </td>
      <td>
      {{windSpeed}}
      </td>
    </tr>
      {{/each}}
  </tbody>
</table>